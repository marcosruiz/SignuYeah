<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/layoutPdf"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="markens.signu.activities.pdf.PdfActivity">

    <FrameLayout
        android:id="@+id/fragment_container_pdf"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@id/bottom_navigation_pdf"/>


    <android.support.design.widget.BottomNavigationView
        android:id="@+id/bottom_navigation_pdf"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        app:menu="@menu/bottom_navigation_pdf"/>


</RelativeLayout>
